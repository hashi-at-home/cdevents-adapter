{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cdevents-adapter-test",
  "compatibility_date": "2025-08-03",
  "main": "./src/index.tsx",
  "workers_dev": false,
  "routes": [
    {
      "custom_domain": true,
      "pattern": "cdevents.brucellino.dev"
    }
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "r2_buckets": [
    {
      "binding": "EVENTS_BUCKET",
      "bucket_name": "brucellino-cd-events"
    }
  ]
}
